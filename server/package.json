{
  "name": "server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "dev": "env-cmd -f ../.env nodemon",
    "start": "node build/index.js",
    "prepush": "yarn build",
    "precommit": "yarn lint && yarn prettier",
    "prettier": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "clean": "rm -rf ./build && rm -rf ./node_modules",
    "launch-prod": "env-cmd -f ../.env yarn start"
  },
  "dependencies": {
    "axios": "1.1.3",
    "bcrypt": "5.1.0",
    "joi": "17.6.4",
    "jsonwebtoken": "8.5.1",
    "luxon": "3.0.4",
    "mongoose": "6.6.7",
    "swagger-jsdoc": "6.2.5",
    "swagger-ui-express": "4.5.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "4.7.1",
    "ws": "8.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5",
    "@types/jsonwebtoken": "^8",
    "@types/luxon": "^3",
    "@types/node": "^18.11.0",
    "@types/swagger-jsdoc": "^6",
    "@types/swagger-ui-express": "^4",
    "@types/ws": "8.5.3",
    "env-cmd": "10.1.0",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "shared": "1.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
